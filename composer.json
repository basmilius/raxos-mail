{
    "name": "raxos/mail",
    "description": "Mailing system for Raxos.",
    "license": "mit",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "library",
    "authors": [
        {
            "name": "Bas Milius",
            "email": "bas@mili.us"
        }
    ],
    "autoload": {
        "psr-4": {
            "Raxos\\Mail\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "8.4"
        },
        "allow-plugins": {
            "php-http/discovery": false
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../*",
            "symlink": true
        }
    ],
    "require": {
        "jetbrains/phpstorm-attributes": "^1.2",
        "mailgun/mailgun-php": "^v4.3",
        "phpmailer/phpmailer": "^v6.10",
        "raxos/foundation": "*",
        "wildbit/postmark-php": "^v7.0"
    }
}
